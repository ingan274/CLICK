        <label for="location">Location</label>
        <select class="form-control" id="location">
            {{#each location}}
            <option>{{dataValues.city}}}</option>
            {{/each}}
        </select>
        <label for="heightFtMin heightInchMin ">Min Height:</label>
        <select class="form-control" id="heightFtMin">
            <option selected>Min. (Ft)</option>
            <option data-value="4">4'</option>
            <option data-value="5">5'</option>
            <option data-value="6">6'</option>
            <option data-value="7">7'</option>
        </select>
        <select class="form-control" id="heightInchMin">
            <option data-value="0" selected>Min. (Inch)</option>
            <option data-value="0">0''</option>
            <option data-value="1">1''</option>
            <option data-value="2">2''</option>
