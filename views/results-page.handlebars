        <label for="heightFtMax heightInchMax ">Max Height:</label>
        <select class="form-control" id="heightFtMax">
            <option selected>Max. (Ft)</option>
            <option data-value="4">4'</option>
            <option data-value="5">5'</option>
            <option data-value="6">6'</option>
            <option data-value="7">7'</option>
