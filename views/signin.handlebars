<div class="signinDIV">
    
    <form id="signin" name="signin" method="post" action="signin">
        <h1 class="titleSign">Sign In</h1> 
        <label for="email">Email Address</label>
        <input class="text emailEntry" name="email" type="text" />
        <label for="password">Password</label>
        <input name="password" type="password" />
        <input class="btn" id="signInBtn" type="submit" value="Sign In" />
    </form>


    <div>
        <button id="sign-upBtn" class="btn createAccount"> Create an Account </button>
    </div>
</div>

<script type="text/javascript">
    $("#sign-upBtn").on("click", function () {
        $.ajax({
            method: "GET",
            url: "/signup",
        })
            .then(function () {
                window.location.href = "/signup";
            });
    });
</script>